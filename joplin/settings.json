{
	"$schema": "https://joplinapp.org/schema/settings.json",
	"editor.codeView": true,
	"sync.target": 3,
	"richTextBannerDismissed": true,
	"editor.pluginCompatibilityBannerDismissedFor": [
		"com.whatever.quick-links",
		"io.github.personalizedrefrigerator.joplin-vimrc",
		"joplin-plugin-conflict-resolution",
		"joplin.plugin.ambrt.fold-cm",
		"joplin.plugin.cmoptions",
		"org.joplinapp.plugins.AbcSheetMusic",
		"outline",
		"plugin.calebjohn.rich-markdown",
		"shufo.markdown-prettier",
		"ylc395.betterMarkdownViewer",
		"io.github.jackgruber.backup"
	],
	"locale": "en_US",
	"theme": 2,
	"themeAutoDetect": false,
	"layoutButtonSequence": 0,
	"notes.sortOrder.field": "order",
	"notes.sortOrder.reverse": true,
	"notes.perFieldReverse": {
		"user_updated_time": true,
		"user_created_time": true,
		"title": false,
		"order": true,
		"todo_due": true,
		"todo_completed": true
	},
	"newNoteFocus": "title",
	"markdown.plugin.softbreaks": true,
	"markdown.plugin.typographer": true,
	"markdown.plugin.fountain": true,
	"markdown.plugin.footnote": true,
	"markdown.plugin.sub": true,
	"markdown.plugin.sup": true,
	"markdown.plugin.insert": true,
	"startMinimized": true,
	"style.editor.fontSize": 12,
	"style.editor.fontFamily": "MesloLGS NF",
	"style.editor.monospaceFontFamily": "MesloLGS NF",
	"style.editor.contentMaxWidth": 0,
	"ui.layout": {
		"key": "root",
		"children": [
			{
				"key": "sideBar",
				"visible": true,
				"width": 146
			},
			{
				"key": "noteList",
				"visible": true,
				"width": 195
			},
			{
				"key": "plugin-view-outline-outline.panel",
				"context": {
					"pluginId": "outline"
				},
				"visible": true,
				"width": 215
			},
			{
				"key": "editor",
				"visible": true
			}
		],
		"visible": true
	},
	"autoUpdateEnabled": true,
	"clipperServer.autoStart": true,
	"noteVisiblePanes": [
		"editor",
		"viewer"
	],
	"editor.keyboardMode": "vim",
	"editor.spellcheckBeta": true,
	"net.ignoreTlsErrors": false,
	"api.token": "55f8fae36853c0317fb8fdf395bf06d2e73aac21a7429a1d668a26f1662ac7a27598b574d863600c664bb0534aaa7fca2345f60dd204fcf817c22828f45a25d6",
	"spellChecker.language": "en-US",
	"spellChecker.languages": [
		"en-US"
	]
}
